{% extends '::base.html.twig' %}
{% block title %}New Category{% endblock %}
{% block content %}
<div class="content-wrapper">
    <h1><a href="{{ path('admin_category_index') }}">Categories</a> > Create</h1>

    <form action="{{ path('admin_category_new') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">Create</button>
        </p>
    </form>
</div>
{% endblock %}
