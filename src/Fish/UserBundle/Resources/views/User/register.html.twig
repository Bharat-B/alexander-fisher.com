{% extends '::base.html.twig' %}

{% block content %}

    <div id="register-wrapper">
        {% if error %}
            <div class="error">{{ error.message }}</div>
        {% endif %}

        <form action="{{ path('register') }}" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}

            <input type="submit" value="Register" />
        </form>
    </div>
        
{% endblock %}
